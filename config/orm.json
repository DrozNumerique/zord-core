{
	"UserEntity": {
		"table": "users",
		"fields": [
			"login",
	        "password",
	        "email",
	        "name",
	        "ips",
	        "reset"
        ],
        "key": "login",
        "join": {
        	"UserHasProfileEntity": ["login", "user"],
        	"UserHasIPV4Entity": ["login", "user"],
        	"UserHasRoleEntity": ["login", "user"],
        	"UserHasSessionEntity": ["login", "user"],
        	"UserHasTokenEntity": ["login", "user"]
        },
        "elements": {
        	"UserHasProfileEntity": ["profiles", "user"],
        	"UserHasIPV4Entity": ["addresses", "user"],
        	"UserHasRoleEntity": ["roles", "user"],
        	"UserHasSessionEntity": ["sessions", "user"],
        	"UserHasTokenEntity": ["tokens", "user"]
        }
    },
    "UserHasProfileEntity": {
    	"table": "user_has_profile",
    	"fields": [
    		"id",
    		"user",
    		"date",
    		"profile"
    	],
    	"key": "id"
    },
	"UserHasIPV4Entity": {
		"table": "user_has_ipv4",
		"fields": [
	        "user",
	    	"ip",
	    	"mask",
	    	"include"
        ],
        "key": [
        	"user",
        	"ip"
        ],
        "expr": {
           	"ip": {
           		"get": "INET_NTOA",
           		"set": "INET_ATON"
           	}
        }
    },
	"UserHasRoleEntity": {
		"table": "user_has_role",
		"fields": [
	        "user",
	        "role",
	        "context",
	        "start",
	        "end"
        ],
        "key": [
        	"user",
        	"role",
        	"context"
        ]
    },
	"UserHasSessionEntity": {
		"table": "user_has_session",
		"fields": [
	        "user",
	        "session",
	        "last"
        ],
        "key": "session"
    },
	"UserHasTokenEntity": {
		"table": "user_has_token",
		"fields": [
	        "user",
	        "key",
	        "token",
	        "start"
        ],
        "key": "token"
    },
	"ProcessEntity": {
		"table": "process",
		"fields": [
	        "pid",
	        "class",
	        "user",
	        "lang",
	        "params",
	        "start",
	        "last",
	        "step",
	        "progress"
        ],
        "key": "pid"
    },
	"ProcessHasReportEntity": {
		"table": "process_has_report",
		"fields": [
	        "process",
	        "index",
	        "indent",
	        "style",
	        "message",
	        "newline"
        ],
        "key": [
        	"process",
        	"index"
        ]
    }
}